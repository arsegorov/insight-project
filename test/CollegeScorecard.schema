{
    "description": "Data from College Scorecard reports",
    "pattern": "MERGED%",
    "tables": [
        {
            "table": "inst_basic_info",
            "fields": {
                "inst_id": { "type": "integer", "column": 1 },
                "inst_name": { "type": "text", "column": 4, "nullable": false },

                "inst_city": { "type": "text", "column": 5, "nullable": false },
                "inst_state": { "type": "state2", "column": 6, "nullable": false },
                "inst_zip": { "type": "zip", "column": 7, "nullable": false },

                "inst_hdegree": {
                    "type": "enum",
                    "values":
                        {
                            "0": "Non-degree-granting",
                            "1": "Certificate",
                            "2": "Associate",
                            "3": "Bachelor's",
                            "4": "Graduate"
                        },
                    "column": 16,
                    "nullable": false },

                "inst_lat": { "type": "real", "column": 22},
                "inst_lon": { "type": "real", "column": 23}
            },
            "primary_key": ["inst_id"]
        },
        {
            "table": "inst_enrollment",
            "fields": {
                "inst_id": { "type": "integer", "column": 1, "ref": "inst_basic_info(inst_id)" },
                "year": { "type": "smallint", "column": 0 },
                "inst_ug_enrlmt": { "type": "integer", "range": "0:", "column": 291}
            },
            "primary_key": ["inst_id", "year"]
        }
    ]
}
